               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox fx:id="latersBox" alignment="CENTER_LEFT" layoutX="15.0" layoutY="327.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="45.0" spacing="10.0" styleClass="sideBox">
               <children>
                  <Button id="latersButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#loadLaters" onMouseClicked="#loadLaters" prefHeight="30.0" prefWidth="30.0" styleClass="sideButton" />
                  <Label fx:id="laterLabel" prefHeight="15.0" prefWidth="164.0" text="Watch Later" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox fx:id="likedBox" alignment="CENTER_LEFT" layoutX="15.0" layoutY="373.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="45.0" spacing="10.0" styleClass="sideBox">
               <children>
                  <Button id="likedButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#loadLikes" onMouseClicked="#loadLikes" prefHeight="30.0" prefWidth="30.0" styleClass="sideButton" />
