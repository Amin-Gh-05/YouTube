            <Button id="moreButton" fx:id="moreButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#slideSidebar" prefHeight="30.0" prefWidth="40.0" styleClass="actionButton" />
            <ImageView fx:id="mainLogo" fitHeight="35.0" fitWidth="181.0" onMouseClicked="#refreshAll" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/logo.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
            <TextField id="searchBox" fx:id="searchBox" alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" onAction="#searchAll" prefHeight="35.0" prefWidth="881.0" promptText="Search Your Title Here" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Roboto" size="12.0" />
               </font>
            </TextField>
            <Button id="searchButton" fx:id="searchButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#searchAll" prefHeight="35.0" prefWidth="35.0" styleClass="actionButton">
               <font>
                  <Font name="Roboto" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button id="signInButton" fx:id="signInButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#signIn" prefHeight="35.0" prefWidth="60.0" styleClass="actionButton" text="Sign In">
